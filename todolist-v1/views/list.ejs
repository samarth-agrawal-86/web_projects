<%- include('header'); -%>
<body>
  <div class="box" id="heading">
    <h1><%= page %> List</h1>
  </div>

  <div class="box">
    <% for (var i = 0; i < listItems.length; i++) {const item=listItems[i]; %>
    <div class="item">
      <input type="checkbox" name="" id="" />
      <p><%=item%></p>
    </div>
    <% } %>

    <form class="item" action="/" method="post">
      <input
        type="text"
        name="addItem"
        placeholder="Enter Task"
        autocomplete="off"
      />
      <button type="submit" name="button" value="<%=page%>">+</button>
    </form>
  </div>
</body>

<%- include('footer'); -%>
